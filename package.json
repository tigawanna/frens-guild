{
  "name": "express-api-starter-ts",
  "version": "1.2.0",
  "description": " A basic starter for an express.js API with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "vercel-build": "prisma generate --no-engine",
    
    "build": "tsc && tsc-alias",
    "start:dist": "node dist/src/index.js",
    
    "tsup:build": "npx prisma generate && tsup src/index.ts",
    "tsup:start": "node dist/index.js",
    
    "lint": "eslint --fix src test",
    "test": "jest",
    "db:generate": "prisma generate",
    "db:prod": "prisma generate --no-engine",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@pothos/core": "^4.5.1",
    "@pothos/plugin-prisma": "^4.5.0",
    "@pothos/plugin-relay": "^4.4.1",
    "@prisma/client": "^6.5.0",
    "better-auth": "^1.2.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.13.2",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "request-ip": "^3.3.0",
    "tsc-alias": "^1.8.15",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.13",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prisma": "^6.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.6.3"
  }
}
